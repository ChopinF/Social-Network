<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: ghostwhite;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proiectmap.socialmap.controller.AddUserController">
    <children>
        <GridPane hgap="5.0" layoutX="50.0" prefHeight="250.0" prefWidth="373.0" vgap="5.0"
                  AnchorPane.bottomAnchor="140.0" AnchorPane.leftAnchor="50.0"
                  AnchorPane.rightAnchor="49.0" AnchorPane.topAnchor="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="208.5" minWidth="10.0" prefWidth="120.5" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="237.5" minWidth="10.0" prefWidth="237.5" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Id User" />
                <Label text="First Name" GridPane.rowIndex="1" />
                <Label text="Last Name" GridPane.rowIndex="2" />
                <Label text="Email" GridPane.rowIndex="3" />
                <Label text="Password" GridPane.rowIndex="4" />

                <TextField fx:id="textFieldId" GridPane.columnIndex="1" />
                <TextField fx:id="textFieldFirstName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="textFieldLastName" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="textFieldEmail" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <PasswordField fx:id="textFieldPassword" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            </children>
        </GridPane>
        <ButtonBar layoutX="250.0" layoutY="320.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="20.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#handleSave" style="-fx-background-color: gainsboro; -fx-border-color: black;" text="Save">
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </padding>
                </Button>
                <Button mnemonicParsing="false" onAction="#handleCancel" style="-fx-background-color: gainsboro; -fx-border-color: black;" text="Cancel" />
            </buttons>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </ButtonBar>
    </children>
</AnchorPane>